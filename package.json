{
  "name": "html-css-essential-extensions-pack",
  "displayName": "HTML/CSS Essential Extensions Pack by @keiwsh",
  "description": "A collection of essential HTML/CSS extensions for Visual Studio Code.",
  "version": "0.0.3",
  "publisher": "keiwsh",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "html-css-essential-extensions-pack.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "extensionPack": [
    "esbenp.prettier-vscode",
    "formulahendry.auto-rename-tag",
    "naumovs.color-highlight",
    "pranaygp.vscode-css-peek",
    "vincaslt.highlight-matching-tag",
    "Zignd.html-css-class-completion",
    "bradgashler.htmltagwrap",
    "ritwickdey.liveserver",
    "vscode-icons-team.vscode-icons"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.88.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keiwsh/html_css_essential_extension_vscode/"
  }
}
